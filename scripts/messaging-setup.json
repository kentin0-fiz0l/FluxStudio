{
  "name": "flux-studio-messaging",
  "version": "1.0.0",
  "description": "Enhanced package.json configuration for Flux Studio Messaging System",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:performance": "node scripts/performance-tests.js",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx}",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "ws:start": "node server/websocket-server.js",
    "ws:dev": "nodemon server/websocket-server.js",
    "messaging:test": "vitest run src/tests/messaging.test.tsx",
    "messaging:build": "vite build --outDir dist/messaging",
    "messaging:preview": "vite preview --port 3000",
    "docker:build": "docker build -t flux-studio:latest .",
    "docker:run": "docker run -p 3000:80 flux-studio:latest",
    "docker:compose": "docker-compose -f docker-compose.messaging.yml up",
    "docker:compose:down": "docker-compose -f docker-compose.messaging.yml down",
    "deploy:staging": "vercel --env staging",
    "deploy:production": "vercel --prod",
    "postinstall": "patch-package",
    "clean": "rm -rf dist node_modules/.vite",
    "health:check": "node scripts/health-check.js",
    "monitoring:start": "node scripts/start-monitoring.js"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-oauth/google": "^0.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "embla-carousel-react": "^8.6.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.18",
    "fuse.js": "^7.1.0",
    "google-auth-library": "^10.3.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.487.0",
    "multer": "^2.0.2",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.5.2",
    "react-hook-form": "^7.55.0",
    "react-resizable-panels": "^2.1.7",
    "react-router-dom": "^6.28.0",
    "recharts": "^2.15.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.3",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "use-debounce": "^10.0.6",
    "vaul": "^1.1.2",
    "redis": "^4.7.0",
    "ioredis": "^5.4.1",
    "ws": "^8.18.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "helmet": "^8.0.0",
    "rate-limiter-flexible": "^5.0.3",
    "sharp": "^0.33.5",
    "sanitize-html": "^2.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "@playwright/test": "^1.50.0",
    "eslint": "^9.16.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vite": "6.3.5",
    "vitest": "^2.1.8",
    "patch-package": "^8.0.0",
    "nodemon": "^3.1.9",
    "vite-bundle-analyzer": "^0.12.1",
    "size-limit": "^11.1.6",
    "@size-limit/preset-app": "^11.1.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "size-limit": [
    {
      "path": "dist/assets/*.js",
      "limit": "500 KB"
    },
    {
      "path": "dist/assets/*.css",
      "limit": "50 KB"
    }
  ],
  "keywords": [
    "messaging",
    "collaboration",
    "design",
    "real-time",
    "ai",
    "automation",
    "websocket",
    "react",
    "typescript",
    "flux-studio"
  ],
  "author": "Flux Studio Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/FluxStudio.git"
  },
  "homepage": "https://fluxstudio.com",
  "bugs": {
    "url": "https://github.com/your-org/FluxStudio/issues"
  }
}